  <project path="prebuilts/eclipse" name="platform/prebuilts/eclipse" revision="master"/>
  <project path="prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.11-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.11-4.8" revision="master" />
