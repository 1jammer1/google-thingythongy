<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/" />
  <default revision="master"
           remote="aosp"
           sync-j="4" />

  <project path="tools/buildSrc" name="platform/tools/buildSrc">
    <copyfile src="base/build.gradle"      dest="tools/build.gradle" />
    <copyfile src="base/settings.gradle"   dest="tools/settings.gradle" />
    <copyfile src="base/gradlew"           dest="tools/gradlew" />
    <copyfile src="base/gradlew.bat"       dest="tools/gradlew.bat" />
    <copyfile src="base/gradle.properties" dest="tools/gradle.properties" />
  </project>

  <project path="external/qemu" name="platform/external/qemu" />
  <project path="prebuilts/android-emulator" name="platform/prebuilts/android-emulator" />
  <project path="prebuilts/tools" name="platform/prebuilts/tools" />
  <project path="sdk" name="platform/sdk" />
  <project path="tools/adt/eclipse" name="platform/tools/adt/eclipse" />
  <project path="tools/adt/idea" name="platform/tools/adt/idea" />
  <project path="tools/base" name="platform/tools/base" />
  <project path="tools/emulator" name="platform/tools/emulator" />
  <project path="tools/external/fat32lib" name="platform/tools/external/fat32lib" />
  <project path="tools/gradle" name="platform/tools/gradle" />
  <project path="tools/idea" name="platform/tools/idea" />
  <project path="tools/studio/cloud" name="platform/tools/studio/cloud" />
  <project path="tools/swt" name="platform/tools/swt" />

  <!-- these projects are all use the master branch -->
  <project path="external/ant-glob" name="platform/external/ant-glob" revision="master" />
  <project path="external/eclipse-windowbuilder" name="platform/external/eclipse-windowbuilder" revision="master" />
  <project path="external/emma" name="platform/external/emma" revision="master"/>
  <project path="external/gtest" name="platform/external/gtest" revision="master" />
  <project path="prebuilts/clang/darwin-x86/host/3.5" name="platform/prebuilts/clang/darwin-x86/host/3.5" revision="master" />
  <project path="prebuilts/eclipse-build-deps" name="platform/prebuilts/eclipse-build-deps" revision="master" />
