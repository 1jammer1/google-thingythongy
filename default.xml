<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/" />

  <default revision="master"
           remote="aosp"
           sync-j="4" />

  <project path="bionic" name="platform/bionic" />
